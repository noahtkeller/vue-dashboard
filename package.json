{
  "name": "dashboard",
  "version": "0.0.0",
  "type": "module",
  "files": ["dist"],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@material/elevation": "^14.0.0",
    "@material/theme": "^14.0.0",
    "@mdi/font": "^7.2.96",
    "@noahtkeller/common-backend": "github:noahtkeller/node-common-backend",
    "@noahtkeller/common-strings": "github:noahtkeller/node-common-strings",
    "@noahtkeller/md-theme": "github:noahtkeller/sass-md-theme",
    "core-js": "^3.31.0",
    "node-sass-json-importer": "^4.3.0",
    "pinia": "^2.0.23",
    "sass": "^1.56.1",
    "vite-plugin-vuetify": "^1.0.0",
    "vue": "^3.2.45",
    "vue-i18n-plugin": "github:noahtkeller/vue-i18n-plugin",
    "vue-oidc-plugin": "github:noahtkeller/vue-oidc-plugin",
    "vue-router": "^4.1.5",
    "vuetify": "^3.0.1",
    "webtorrent": "^2.1.11"
  },
  "types": "./env.d.ts",
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vite-plugin-node-polyfills": "^0.9.0",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
